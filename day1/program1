name=input("Enter the name")
branch=input("Enter the branch")
coll=input("Enter the college")
age=int(input("Enter the age"))
gen=input("Enter the gender")
print("The name of the student",name)
print("The branch of the student",branch)
print("The name of the college",coll)
print("The age of the student",age)
print("The gender of the student",gen)
